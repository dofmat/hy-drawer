{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-console": "off",
    "no-else-return": "off",
    "no-multi-assign": "off",
    "no-underscore-dangle": "off",
    "no-mixed-operators": "off",
    "import/prefer-default-export": "off",
  },
}
